{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  // "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "camelcase": "warn",
    "arrow-body-style": "off",
    "import/extensions": "off",
    "object-curly-newline": "off"
  }
}
